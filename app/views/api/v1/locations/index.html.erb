<p id="notice"><%= notice %></p>

<h1>Api V1 Locations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Total</th>
      <th>White total</th>
      <th>Latinx total</th>
      <th>Black total</th>
      <th>Black pop</th>
      <th>Black deaths</th>
      <th>Black likely</th>
      <th>Latinx likely</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_locations.each do |api_v1_location| %>
      <tr>
        <td><%= api_v1_location.name %></td>
        <td><%= api_v1_location.total %></td>
        <td><%= api_v1_location.white_total %></td>
        <td><%= api_v1_location.latinx_total %></td>
        <td><%= api_v1_location.black_total %></td>
        <td><%= api_v1_location.black_pop %></td>
        <td><%= api_v1_location.black_deaths %></td>
        <td><%= api_v1_location.black_likely %></td>
        <td><%= api_v1_location.latinx_likely %></td>
        <td><%= link_to 'Show', api_v1_location %></td>
        <td><%= link_to 'Edit', edit_api_v1_location_path(api_v1_location) %></td>
        <td><%= link_to 'Destroy', api_v1_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Location', new_api_v1_location_path %>
