<p id="notice"><%= notice %></p>

<h1>Api V1 Victims</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>When</th>
      <th>Place</th>
      <th>Details</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_victims.each do |api_v1_victim| %>
      <tr>
        <td><%= api_v1_victim.name %></td>
        <td><%= api_v1_victim.age %></td>
        <td><%= api_v1_victim.when %></td>
        <td><%= api_v1_victim.place %></td>
        <td><%= api_v1_victim.details %></td>
        <td><img height='100px' width='100px' src=<%= api_v1_victim.photo %> /> </td>
        <td><%= link_to 'Show', api_v1_victim %></td>
        <td><%= link_to 'Edit', edit_api_v1_victim_path(api_v1_victim) %></td>
        <td><%= link_to 'Destroy', api_v1_victim, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Victim', new_api_v1_victim_path %>
